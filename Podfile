platform :ios, '11.0'

target 'VibeCheckBot' do
  use_frameworks!

  # Pods for VibeCheckBot
  pod 'OAuthSwift', '~> 2.0.0'
  pod 'KeychainAccess'
  
  # Add the Firebase pod for Google Analytics
  pod 'Firebase/Analytics'
  pod 'Fabric'
  pod 'Crashlytics'

  target 'VibeCheckBotTests' do
    inherit! :search_paths
  end

end
